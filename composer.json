{
	"name": "yauhenko/rest-bundle",
	"version": "0.3.0",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "REST and Docs for Symfony 5",
	"license": "proprietary",
	"require": {
		"php": "^8.0"
	},
	"require-dev": {
		"doctrine/annotations": "^1.0",
		"doctrine/doctrine-bundle": "^2.2",
		"doctrine/doctrine-migrations-bundle": "^3.0",
		"symfony/security-bundle": "5.3.*",
		"doctrine/orm": "^2.8",
		"sensio/framework-extra-bundle": "^6.0",
		"symfony/validator": "^5.3",
		"symfony/serializer": "^5.3"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yauhenko\\RestBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "5.3.*"
		}
	}
}
