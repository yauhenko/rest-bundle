{
	"name": "yauhenko/rest-bundle",
	"version": "0.8.0",
	"type": "symfony-bundle",
	"minimum-stability": "stable",
	"description": "REST and Docs for Symfony 6",
	"license": "proprietary",
	"require": {
		"php": "^8.1"
	},
	"require-dev": {
		"doctrine/doctrine-bundle": "^2.2",
		"doctrine/doctrine-migrations-bundle": "^3.0",
		"symfony/security-bundle": "6.0.*",
		"doctrine/orm": "^2.9",
		"sensio/framework-extra-bundle": "^6.0",
		"symfony/validator": "^6.0",
		"symfony/serializer": "^6.0"
	},
	"config": {
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"Yauhenko\\RestBundle\\": "src/"
		}
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "6.0.*"
		}
	}
}
